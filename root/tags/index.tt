<h1>Tags</h1>

<div class="tag-list overlay">
    [% FOREACH tag IN tags.nsort("card_count").reverse %]
        <p>
            <span class="tag">
                [% FILTER html %]
                    [% tag.name %]
                [% END %]
            </span>
            ([% tag.card_count %]x)
        </p>
    [% END %]
</div>
